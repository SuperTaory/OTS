<bean id="UserService" class="com.ttc.ssm.service.impl.UserServiceImpl"/>
<!-- 试卷管理的service -->
<bean id="TestService" class="com.ttc.ssm.service.impl.TestServiceImpl"/>
<!-- 测试管理的service -->
<bean id="DoTestService" class="com.ttc.ssm.service.impl.DoTestServiceImpl"/>
